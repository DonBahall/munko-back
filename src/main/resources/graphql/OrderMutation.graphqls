extend type Mutation {
    addItemInBasket(userId: Int, funkoId: Int!): OrderItem
    deleteItemInBasket(userId: Int, itemId: Int): Boolean
    updateItemInBasket(userId: Int, funkoId: Int, amount: Int): OrderItem
}

input OrderItemInput{
    id: Int
    funkoId: Int
    amount: Int
}

enum Status{
    PENDING
    PAID
    FINISHED
}