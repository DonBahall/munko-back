extend type Mutation {
    createOrder(sessionID: String, userId: Int, funkoId: Int!): Order
}

input OrderInput{
    id: Int
    userId: UserInput
    sessionID: String
    orderItems: [OrderItemInput]
    status: Status
}

input OrderItemInput{
    id: Int
    img: String
    name: String
    amount: Int
    pricePerItem: Int
    order_id: Int
}

enum Status{
    PENDING
    PAID
    FINISHED
}